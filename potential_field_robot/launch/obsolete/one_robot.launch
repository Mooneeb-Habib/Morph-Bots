<launch>
    <arg name="robot_name"/>
    <arg name="init_pose"/>
    <arg name="NOR" default="1"/>

    <node name="spawn_minibot_model" pkg="gazebo_ros" type="spawn_model"
     args="$(arg init_pose) -urdf -param /robot_description -model $(arg robot_name)"
     respawn="false" output="screen" />

    <!-- start node -->

    <node pkg="potential_field_robot" type="robot_state_pub.py" name="$(arg node_name_arg)" args="robot_name init_pose NOR"/>

</launch>
